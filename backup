--
-- PostgreSQL database cluster dump
--

SET default_transaction_read_only = off;

SET client_encoding = 'SQL_ASCII';
SET standard_conforming_strings = on;

--
-- Roles
--

CREATE ROLE postgres;
ALTER ROLE postgres WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN REPLICATION;
CREATE ROLE ubuntu;
ALTER ROLE ubuntu WITH SUPERUSER INHERIT CREATEROLE CREATEDB LOGIN NOREPLICATION;






--
-- Database creation
--

CREATE DATABASE "DevcampPortfolio_development" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "DevcampPortfolio_test" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
