CREATE DATABASE "MApp_test" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "RoutingApp_development" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "RoutingApp_test" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "creating-a-ruby-gem_development" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "creating-a-ruby-gem_test" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "myProject_development" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "myProject_test" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "sample-project_development" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
CREATE DATABASE "sample-project_test" WITH TEMPLATE = template0 OWNER = ubuntu ENCODING = 'UTF8';
